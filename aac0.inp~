%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%
% abc0.inp
%
% Creates nodes to be used as inputs to the model.
% This is for a long stimuli (long sequences)
% The total number of nodes is 81 (9x9)
% Any arbitrary input can be configured by turning
% on nodes in a timeline.
%
% The segment below corresponds to:
% A. 500-ms tonal pattern
% B. 300-ms intrasegmental period
% C. 500-ms tonal pattern
% D. 300-ms intrasegmental period
% E. 500-ms tonal pattern
%
% 1 simulation timestep = 5 milliseconds
%
% Antonio Ulloa
% Fri Aug 23 12:11:44 EDT 2002
%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%


%----------------------------------------------------
% first segment of sequence
%----------------------------------------------------
set (MGNs, 81)          % Turn ON MGN, the Input
{
  Node activation {ALL 0.0 }
  Node activation { ([1,31] 1.00) }
  Node activation { ([1,32] 1.00) }
}
Run 10

%----------------------------------------------------
set (MGNs, 81)          % Turn ON MGN, the Input
{ 
  Node activation {ALL 0.0 }
  Node activation { ([1,32] 1.00) }
  Node activation { ([1,33] 1.00) }
}
Run 10

